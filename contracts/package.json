{
  "name": "dao-vote-contracts",
  "private": false,
  "version": "0.0.0",
  "scripts": {
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .ts,.tsx -f codeframe",
    "clean": "rimraf ./dist/",
    "prebuild": "npm run clean",
    "build-ts": "tsc --skipLibCheck -p ./tsconfig.json",
    "build-tact": "tact --config ./tact.config.json",
    "test": "mocha --timeout 120000 --exit test/*-test.ts",
    "e2e": "mocha --timeout 120000 --exit test/e2e.ts",
    "deploy": "npm run build-ts && node ./dist/deploy/deploy.js"
  },
  "dependencies": {
    "@orbs-network/ton-access": "^2.2.2",
    "@tact-lang/compiler": "^1.0.0",
    "bignumber.js": "^9.1.1",
    "buffer": "^6.0.3",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "4.18.2",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "ton": "^13.4.1"
  },
  "devDependencies": {
    "@tact-lang/emulator": "^3.2.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.4",
    "chai-bn": "^0.3.1",
    "dotenv": "^16.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "16.x"
  },
  "mocha": {
    "require": [
      "chai",
      "ts-node/register"
    ],
    "timeout": 20000
  }
}
